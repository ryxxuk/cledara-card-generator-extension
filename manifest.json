{
  "name": "Cledara Automation Tools",
  "description": "Automate simple tasks in Cledara",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://*.cledara.com/*"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
