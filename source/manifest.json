{
  "name": "Cledara Automation Tools",
  "description": "Automate simple tasks in Cledara",
  "version": "1.3",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting", "identity", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Celdara Automation"
  },
  "content_scripts": [
    {
      "matches": ["https://*.cledara.com/*"],
      "js": ["content-script.js"]
    }
  ],
  "key": "",
  "oauth2": {
    "client_id": "432980324370-3glm82u6f1rgbhj1a6g1qe51u3ni39fs.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/cloud-platform"
    ]
  },
  "icons": {
    "128": "icon.png"
  }
}
